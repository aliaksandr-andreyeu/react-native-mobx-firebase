{
  "name": "react-native-mobx-firebase",
  "version": "0.1.0",
  "description": "React Native boilerplate with React Navigation, MobX and Firebase",
  "author": "Aliaksandr Andreyeu <andreyeu.aliaksandr@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/aliaksandr-andreyeu/react-native-mobx-firebase",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliaksandr-andreyeu/react-native-mobx-firebase.git"
  },
  "main": "index.js",
  "scripts": {
    "dev:port:reverse": "adb reverse tcp:3000 tcp:3000",
    "react:devtools:port:reverse": "adb reverse tcp:8097 tcp:8097",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "npm run dev:port:reverse && npm run react:devtools:port:reverse && react-native start --reset-cache",
    "test": "jest --coverage",
    "postinstall": "husky install && npx husky add .husky/pre-commit \"npm run precommit\"",
    "precommit": "lint-staged",
    "format": "prettier --write \"./**/*.{js,json}\"",
    "lint:js": "eslint \"./**/*.js\"",
    "lint:js:fix": "eslint --fix \"./**/*.js\"",
    "lint": "npm run format && npm run lint:js",
    "lint:fix": "npm run format && npm run lint:js:fix",
    "build:aab": "cd android && ./gradlew --stop && ./gradlew --stacktrace --no-daemon bundleRelease",
    "build:apk": "cd android && ./gradlew --stop && ./gradlew --stacktrace --no-daemon assembleRelease",
    "android:clean": "cd android && ./gradlew --stop && ./gradlew clean",
    "install:apk": "cd android && ./gradlew --stop && adb install -r ./app/build/outputs/apk/release/app-release.apk",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "logcat:clean": "adb logcat -c",
    "logcat:crash": "adb logcat -b crash"
  },
  "dependencies": {
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-firebase/analytics": "^17.5.0",
    "@react-native-firebase/app": "^17.5.0",
    "@react-native-firebase/crashlytics": "^17.5.0",
    "@react-navigation/bottom-tabs": "^6.5.6",
    "@react-navigation/drawer": "^6.6.1",
    "@react-navigation/native": "^6.1.5",
    "@react-navigation/stack": "^6.3.15",
    "axios": "^1.4.0",
    "mobx": "^6.8.0",
    "mobx-log": "^2.1.1",
    "mobx-react": "^7.6.0",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "12.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.8.3",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0"
  }
}
